<table class="table table-striped" border="1" style="background-color: #f5f5f5; color: #000000;">

    <thead>
    <tr>
        <th>email</th>
        <th>name</th>
        <th>lastname</th>
        <th>tel</th>
        <th>Image</th>
    </tr>
    </thead>
    <tbody>
    {% for a in users %}
            <tr>
                <td> {{a.email }}</td>
                <td> {{a.name }} </td>
                <td> {{a.lastname}} </td>
                <td> {{a.tel}} </td>
                <td><img src="{{ asset(a.getImgURL()) }}" alt="{{ a.getImgURL() }}" style="width: 100px;"></td>
               {# {% set img = a.image %}
                <td><img src="{{asset(img)}}" alt="#" class="" style="width:90%;"></td>#}
            </tr>

    {% else %}
        <tr>
            <td colspan="8">no records found</td>
        </tr>
    {% endfor %}
    </tbody>
</table>